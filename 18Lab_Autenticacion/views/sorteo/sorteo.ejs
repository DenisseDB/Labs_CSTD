<%- include('../includes/head.ejs') %> <%- include('../includes/nav.ejs') %>
<h1>Las personas que han participado son</h1>
<% if (nombresSorteo.length > 0) { %>
<div class="collection">
  <% for (let g of nombresSorteo) { %>
  <a href="#!" class="collection-item"> <%= g.nombre %></a>
  <% } %>
</div>
<% } else { %>
<h2>No hay solicitudes previas</h2>
<% } %>
<p>En total estas compitiendo contra <%= clicks %> participantes</p>
<p>La ultima persona en participar fue <%= nombre %></p>
